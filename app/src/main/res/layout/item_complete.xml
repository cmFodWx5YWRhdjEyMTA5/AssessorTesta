<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">



    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cardviewComplete"
        android:layout_margin="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:weightSum="2"
                android:orientation="horizontal"
                >




            <LinearLayout
                android:id="@+id/len1"
                android:weightSum="4"
                android:layout_weight=".8"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/batchname1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Batch Name"
                    android:theme="@style/accountnameStylecompleted"

                    />



                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/totalstudent1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Student"
                    android:theme="@style/accountnameStylecompleted"
                    />
                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/assessment1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Assessment Date"
                    android:theme="@style/accountnameStylecompleted"
                    />
                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/tcname1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tc Name"
                    android:theme="@style/accountnameStylecompleted"
                    />


            </LinearLayout>





            <LinearLayout
                android:weightSum="4"
                android:layout_alignParentRight="true"
                android:orientation="vertical"
                android:layout_weight="1.2"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/batchnameidcomp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Batch Name"
                    android:textSize="14sp"
                    android:textStyle="normal"


                    />



                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/totalstudentidcomp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Student"
                    android:textSize="14sp"
                    android:textStyle="normal"

                    />
                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/assessmentidcomp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Assessment Date"
                    android:textSize="14sp"
                    android:textStyle="normal"

                    />
                <TextView
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:id="@+id/tcnameidcomp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tc Name"
                    android:textSize="14sp"
                    android:textStyle="normal"

                    />


            </LinearLayout>



     </LinearLayout>





        </RelativeLayout>

    </android.support.v7.widget.CardView>

</LinearLayout>




